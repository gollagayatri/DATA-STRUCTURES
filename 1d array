#include<stdio.h>
void main()
{
    int i,j,c=0,n,a[100];
    printf("enter the size of an array");
    scanf("%d",&n);
    printf("enter the elements");
    for(i=0;i<=n-1;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n-1;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(a[i]==a[j])
            c=c+1;
        }
    }
    printf("%d",c);
    
}
